	AFTER_DOCTYPE_PUBLIC_IDENTIFIER;
	BETWEEN_DOCTYPE_PUBLIC_AND_SYSTEM_IDENTIFIERS;
	AFTER_DOCTYPE_SYSTEM_KEYWORD;
	BEFORE_DOCTYPE_SYSTEM_IDENTIFIER;
